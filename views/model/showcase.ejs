<div class="container">
    <div class="row">
        <%showcaseList.forEach(function (curData, index){%>
        <%
            var cActive=index===0? 'active':null;
        %>
        <div class="col-md-4 col-xs-6">
            <div class="showcase">
                <div class="showcase_time">
                    <span class="pull-right">
                        <a href="<%=curData.url%>" class=" text-center right"><%=curData.tags%></a>
                    </span>
                    <%=curData.createTime%>
                </div>
                <div class="showcase_img">
                    <img class="" src="<%=curData.imgUrl%>" alt="">
                </div>
                <div class="showcase_text">
                    <h4>
                        <a href="<%=curData.url%>"><%=curData.title%></a>
                    </h4>
                    <div class="showcase_info"><%=curData.info%></div>
                </div>
            </div>
        </div>
        <%});%>
    </div>
</div>