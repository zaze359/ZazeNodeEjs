<div class="showcase">
    <div class="showcase_time">
                    <span class="pull-right">
                        <a href="<%= showcaseValue.showcaseTagUrl %>"
                           class=" text-center right"><%= showcaseValue.showcaseTag %></a>
                    </span>
        <%= showcaseValue.showcaseTime %>
    </div>
    <div class="showcase_img">
        <a href="<%= showcaseValue.showcaseTitleUrl %>" target="_blank">
            <img class="" src="<%= showcaseValue.showcaseImg %>" alt="">
        </a>
    </div>
    <div class="showcase_text">
        <h4>
            <a href="<%= showcaseValue.showcaseTitleUrl %>" target="_blank"><%= showcaseValue.showcaseTitle %></a>
        </h4>
        <div class="showcase_info"><%= showcaseValue.showcaseInfo %></div>
    </div>
</div>